config case_sensitive = false timeframe = 7d

| dataset = xdr_data
| filter event_type = ENUM.NETWORK
| filter (dns_query_name contains "api.ipify.org") or (action_remote_port = 7712 and action_remote_ip not contains "10., 192.168., 172.16.")
| fields _time, agent_hostname, dns_query_name, action_remote_port, action_remote_ip
